    <string-array name = "temperaturecaculate">
    	<item>℃ = ( ℉ - 32 )/1.8</item>
    	<item>℉ = ℃*1.8 + 32</item>
    	<item>K = ℃ + 273.15</item>
    	<item>℃ = K - 273.15</item>
    	<item>°Ra = ℃*1.8 + 32 + 459.67</item>
    	<item>℃ = (°Ra - 32 - 459.67)/1.8</item>
    	<item>°Re = ℃*0.8</item>
    	<item>℃ = °Re*1.25</item>
    	<item>℉ = K*1.8 - 459.67</item>
    	<item>K = (℉ + 459.67)/1.8</item>
    	<item>℉ = °Ra - 459.67</item>
    	<item>°Ra = ℉ + 459.67</item>
    	<item>℉ = °Re*2.25 + 32</item>
    	<item>°Re = (℉ - 32)/2.25</item>
    	<item>K = °Ra/1.8</item>
    	<item>°Ra = K*1.8</item>
    	<item>K = °Re*1.25 + 273.15</item>
    	<item>°Re = (K - 273.15)*0.8</item>
    	<item>°Ra = °Re*2.25 + 32 + 459.67</item>
    	<item>°Re = (°Ra - 459.67 - 32)/2.25</item>
    </string-array>
